{
  "roots": [
    "<rootDir>/"
  ],
  "modulePathIgnorePatterns": [
    "<rootDir>/build"
  ],
  "preset": "ts-jest",
  "testEnvironment": "node",
  "moduleFileExtensions": [
    "ts",
    "tsx",
    "js",
    "jsx",
    "json",
    "node"
  ],
  "testMatch": [
    "**/?(*.)+(spec|test)\\.(ts|tsx|js)"
  ],
  "resolver": "jest-ts-webcompat-resolver"
}